{
  "functions": {
    "src/app/api/upload/route.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
